#include <stdio.h>

void update(int *a,int *b) {
    // Complete this function    
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    pa = a + b;
    if(a>b){
        pb = a - b;
    }
    else {
        pb = b - a;
    }
    printf("%d\n%d", pa, pb);

    return 0;
}
